#include<iostream>  //библиотека для базы
#include<string>  //библиотека для строк
using namespace std;  //чтобы не писать лишний раз в выводе std
int main()  //главная функция(голова)
{
	int n, res = 0; //переменные
	cin >> n;  //задаем переменную
	string s, s1; // строки для выполнения шифровки
	cin >> s >> s1; //задаем строки()
	for (int i = 0; i < s.size(); ++i) {              //цикл производящий шифровку
		int i1 = s[i] - 48, i2 = s1[i] - 48;       //шифровка
		if (i2 > i1) 
			swap(i2, i1);   // замена местами при выполнении условия(часть шифровки)
		res += min(abs(i1 - i2), abs(9 - i1) + i2 + 1);  //формула результата(итоговый результат преобразования)
	}
	
	cout << res;  //вывод
	return 0;
}
